diff -ur xkeyboard-config-2.36/symbols/capslock xkeyboard-config-2.36-new/symbols/capslock
--- xkeyboard-config-2.36/symbols/capslock	2022-06-02 00:44:25.000000000 +0200
+++ xkeyboard-config-2.36-new/symbols/capslock	2022-09-06 11:46:07.501778999 +0200
@@ -95,3 +95,14 @@
 xkb_symbols "none" {
     key <CAPS> { [ VoidSymbol ] };
 };
+
+// Use caps lock as F13
+// Shift+Caps lock gives caps lock
+partial hidden function_keys
+xkb_symbols "f13" {
+    key <CAPS> {
+        type[Group1] = "TWO_LEVEL",
+        symbols[Group1] = [ F13, Caps_Lock ],
+        actions[Group1] = [ NoAction(), LockMods(modifiers=Lock) ]
+    };
+};
